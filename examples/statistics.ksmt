defn [mean l] [[/ sum size] l]

defn [median l] {
    := s [size l]
    := d [div s 2]
    if_else [odd? s] [get l d] [div [+ [get l d] [get l [prev d]]] 2]
}

defn [standard_deviation l] {
    := m [mean l]
    := s 0
    for: a l {
        |>= s [+ [squared [- a m]]
    }
    sqrt [/ s [prev [size l]]]
}

defn [skewness l] [/ [* 3 [- [mean l] [median l]]] [standard_deviation l]]